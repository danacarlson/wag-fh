<section id="about">
  <div class="container">
    <div class="row">
        <div class="col-sm-offset-1 col-sm-10 text-center">
            {{ content }}
        </div>
    </div>
    <div class="row text-center">
        {% for about in page.header.about %}
            <div class="col-md-6 group-photo">
                {% if about.image %}
                    <img src="{{ about.image }}">
                {% endif %}
                {% if about.caption %}
                    <p class="text-muted">{{ about.caption }}</p>
                {% endif %}
            </div>
        {% endfor %}
    </div>
</div>
</section>
