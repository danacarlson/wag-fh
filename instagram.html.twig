

{% if feed %}
<div class="instagram-feed inner-embed carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    {% for post in feed|slice(0, count) %}
        <div class="carousel-item item {% if loop.first %}active{% endif %}">
            <a href="{{ post.link }}" target="_blank">
                <img src="{{ post.image }}" alt="">
                <p>{{ post.text }}</p>
            </a>
        </div>
    {% endfor %}
  </div>
</div>
{% endif %}
